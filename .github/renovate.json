{
    
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": ["config:recommended"],
    "assignees": ["acbgbca"],
    "prHourlyLimit": 1,
    "ignoreTests": false,
    "minimumReleaseAge": "3 days",
    "packageRules": [
        {
            "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
            "automerge": true
        },
        {
            "matchDepTypes": ["devDependencies"],
            "automerge": true
        }
    ]
}